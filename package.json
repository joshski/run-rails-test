{
  "name": "run-rails-test",
	"displayName": "run-rails-test",
	"description": "Runs rails test",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
    "onCommand:run-rails-test.runRailsTestForCurrentLine"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
			"command": "run-rails-test.runRailsTestForCurrentLine",
			"title": "Run rails test against current line"
		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.62.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
  },
	"repository": {
		"type": "git",
		"url": "https://github.com/joshski/run-rails-test"
	}
}
